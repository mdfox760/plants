<p id="notice"><%= notice %></p>
<%= link_to 'New Plant', new_plant_path %>
<table class="table">
  <thead>
    <tr>
      <th>Season</th>
      <th>Name</th>
      <th>Care</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plants.each do |plant| %>
      <tr>
        <td><%= plant.season %></td>
        <td><%= plant.name %></td>
        <td><%= plant.care %></td>
        <td><%= link_to 'Show', plant %></td>
        <td><%= link_to 'Edit', edit_plant_path(plant) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
